    <!--combobox-->
</head>
<body class="BlueTheme">
    <div id="outer">
        <div id="header">
            <div class="Logo">
                <a href="/HomeCaseStudy" title="Building Bell">
                    <img src="~/Content/images/logo.png" alt="Building Bell Logo" />
                </a>
                <h1>
                    Building Bell
                </h1>
            </div>
            <!--Top Navigation Bar-->
            @Html.Sitecore().Placeholder("top_nav_bar")
            <div class="BlockBanner01">
                <a href="#" title="Ads banner" target="_blank">
                    <img src="~/Content/images/ads-banner-01.jpg" alt="Ads banner" title="Ads banner" />
                </a>
            </div>
        </div>
        <!--Rendering Body-->
        @Html.Sitecore().Placeholder("main_body")
        <div id="Footer">
            <div id="FooterContent">
                <div class="FooterBlock01">
                    <h3 class="FooterHeading">
                        About
                    </h3>
                    <p>
                        Lorem ipsum dolor sit amet, consectetur adipiscing elit. Integer in velit id metus
                        dignissim tincidunt vitae vitae nulla. In tortor enim, aliquam nec semper in, rhoncus
                        faucibus augue,...
                    </p>
                </div>
                <div class="FooterBlock02">
                    <p class="CopyrightText">
                        &copy; @DateTime.Now.Year Buildingbell &ndash; <a href="mailto:info@buildingbell.com" title="Send mail to info@buildingbell.com">
                            info@buildingbell.com
                        </a> &ndash; Legal information
                    </p>
                </div>
            </div>
        </div>
    </div>
    <script type="text/javascript" src="~/Content/js/jquery-core.js"></script>
    <script type="text/javascript" src="~/Content/js/jquery-ui-core.js"></script>
    <script type="text/javascript" src="~/Content/js/jquery.mediaqueries.js"></script>
    <!--jCarousel-->
    <script type="text/javascript" src="~/Content/js/j-carousel.js"></script>
    <script type="text/javascript" src="~/Content/js/common.js"></script>

    <!--auto complete-->
    <script src="~/Content/js/combobox/jquery.ui.widget.js" type="text/javascript"></script>
    <script src="~/Content/js/combobox/jquery.ui.button.js" type="text/javascript"></script>
    <script src="~/Content/js/combobox/jquery.ui.position.js" type="text/javascript"></script>
    <script src="~/Content/js/combobox/jquery.ui.autocomplete.js" type="text/javascript"></script>
    <script type="text/javascript" src="~/Content/js/combobox/j-mousewheel.js"></script>
    <script type="text/javascript" src="~/Content/js/combobox/jquery.jscrollpane.js"></script>
    <!--auto complete-->
</body>
</html>
